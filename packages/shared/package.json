{
    "name": "@company/shared",
    "version": "0.0.1",
    "description": "Shared typings and common ts files",
    "private": true,
    "scripts": {
        "compile": "tsc --project tsconfig.json",
        "build": "tsc --build",
        "dev": "concurrently \"tsc --watch --project tsconfig.json\" \"tsc-alias -w\"",
        "clean": "rm -rf ./build; rm ./tsconfig.tsbuildinfo || echo"
    },
    "publishConfig": {
    },
    "devDependencies": {
        "@types/node": "16.11.18",
        "tsc-alias": "^1.5.0",
        "concurrently": "^6.5.1",
        "jest": "^27.0.6",
        "ts-jest": "^27.0.5",
        "typescript": "^4.5.4"
    },
    "dependencies": {
    },
    "peerDependencies": {
        "undici": "^5.0.0"
    }
}
